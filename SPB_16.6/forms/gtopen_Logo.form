FILE_TYPE=FORM_DEFN VERSION=2
FORM AUTOGREYTEXT
FIXED
PORT 69 10
HEADER "BMP Logo导入"
POPUP <fontlist_pop>""""
POPUP <layerclass_pop>""""
POPUP <layersubclass_pop>""""
POPUP <brushmode>""""
TILE

GROUP "预览"
GLOC 1 1
GSIZE 40 22
ENDGROUP 
FIELD bmp_show
THUMBNAIL
FLOC 3 5
FSIZE 37 16
ENDFIELD

GROUP "参数"
GLOC 43 1
GSIZE 26 22
ENDGROUP

FIELD layerclass
FLOC 47 4
ENUMSET 18 1
POP layerclass_pop
OPTIONS prettyprint
ENDFIELD

FIELD vis
FLOC 44 7
COLOR 2 1
ENDFIELD

FIELD layersubclass
FLOC 47 7
ENUMSET 18 1
OPTIONS ownerdrawn prettyprint
POP layersubclass_pop
ENDFIELD

TEXT "大小"
TLOC 44 10
ENDTEXT

FIELD pic_size
FLOC 50 10
INTSLIDEBAR 4 2
MIN 1
MAX 50
ENDFIELD

FIELD textset
FLOC 60 10
MENUBUTTON "设置" 3 3
ENDFIELD

TEXT "类型"
TLOC 44 13
ENDTEXT


FIELD neg
FLOC 50 13
CHECKLIST "负片"
ENDFIELD



TEXT
FLOC 1 25
INFO bmpinput 100
FSIZE 45 2
ENDTEXT

FIELD load
FLOC 45 24
MENUBUTTON "1.导入" 9 4
ENDFIELD

FIELD place
FLOC 58 24
MENUBUTTON "2.摆放" 9 4
ENDFIELD

ENDTILE
ENDFORM